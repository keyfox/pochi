{
  "name": "@keyfox/pochi",
  "description": "The MS-IME Japanese Input emulation library for JavaScript",
  "author": "Keyfox",
  "version": "0.2.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/keyfox/pochi.git",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf ./lib/* && tsc --outDir ./lib/",
    "dist": "rm -rf ./dist/* && webpack --config webpack.config.js",
    "test": "mocha -r ts-node/register --ui bdd ./test/**/*.test.ts",
    "cover": "nyc npm run test",
    "lint": "eslint --ext .ts ./src",
    "generate-docs": "rm -rf ./docs/* && typedoc --stripInternal --out ./docs ./src && touch ./docs/.nojekyll"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.9.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "files": [
    "README.md",
    "package.json",
    "lib/",
    "test/"
  ],
  "dependencies": {}
}
