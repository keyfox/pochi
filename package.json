{
  "name": "@keyfox/pochi",
  "description": "The MS-IME Japanese Input emulation library for JavaScript",
  "author": "Keyfox",
  "version": "0.1.0",
  "main": "lib/index.js",
  "repository": "https://github.com/keyfox/pochi.git",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf lib/* && babel src -d lib",
    "dist": "rm -rf dist/* && webpack --config webpack.config.js",
    "test": "mocha --require @babel/register --ui bdd ./test/**/*.test.js",
    "cover": "nyc npm run test",
    "lint": "eslint src",
    "generate-docs": "rm -r docs/* && jsdoc -c .jsdoc.json -d docs -r src"
  },
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/node": "^7",
    "@babel/preset-env": "^7",
    "@babel/register": "^7",
    "babel-loader": "^8.1.0",
    "chai": "^4",
    "clean-jsdoc-theme": "^2",
    "eslint": "^6",
    "eslint-config-prettier": "^6",
    "eslint-plugin-prettier": "^3",
    "jsdoc-import-typedef": "^4.0.0-dev",
    "mocha": "^7",
    "nodemon": "^2",
    "nyc": "^15",
    "prettier": "^2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "files": [
    "README.md",
    "package.json",
    "lib/",
    "test/"
  ],
  "dependencies": {}
}
